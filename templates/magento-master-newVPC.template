                },
                "AdminPassword": {
                    "default": "Administrator password"
                },
                "AdminUserName": {
                    "default": "Administrator user name"
                },
                "AvailabilityZones": {
                    "default": "Availability Zones"
                },
                "DBAllocatedStorage": {
                    "default": "DB Storage Size (in GB)"
                },
                "DBAutoMinorVersionUpgrade": {
                    "default": "Automatic Upgrade To New MySQL Minor Versions"
                },
